<Project Sdk="Microsoft.NET.Sdk">
    
  <PropertyGroup>
    <TargetFrameworks>net9.0;net8.0;net7.0;net6.0;net5.0;net461</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>2.0.$(BuildNum)</Version>
  </PropertyGroup>
    
  <ItemGroup>
    <None Remove="Dummy.txt" />
  </ItemGroup>
    
  <ItemGroup>
    <Content Include="Dummy.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
    
  <ItemGroup>
    <PackageReference Include="JJ.AutoIncrementVersion" Version="2.0.1680" />
  </ItemGroup>

</Project>
