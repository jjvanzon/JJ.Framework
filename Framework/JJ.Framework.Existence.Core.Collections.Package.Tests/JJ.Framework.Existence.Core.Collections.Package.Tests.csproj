<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Version>3.7.$(BuildNum)</Version>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net10.0;net9.0;net8.0;net7.0;net6.0;net5.0</TargetFrameworks>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JJ.Framework.Existence.Core" Version="3.7.4847" />
    <PackageReference Include="JJ.Framework.Existence.Core.Tests.Helpers" Version="3.7.4872" />
    <PackageReference Include="JJ.Framework.Testing.Core" Version="3.7.4847" />
    <PackageReference Include="JJ.Framework.Testing.Legacy" Version="0.250.4847" />
    <PackageReference Include="MSTest" Version="3.7.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\JJ.Framework.Build\JJ.Framework.Build.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\JJ.Framework.Existence.Core.Collections.Tests\**\*.cs" Link="%(RecursiveDir)%(Filename)%(Extension)" />
    <Compile Remove="..\JJ.Framework.Existence.Core.Collections.Tests\obj\**" />
    <Compile Remove="..\JJ.Framework.Existence.Core.Collections.Tests\bin\**" />
  </ItemGroup>

  <Import Project="..\JJ.Framework.PlatformCompatibility.Core\JJ.Framework.PlatformCompatibility.Core.projitems" Label="Shared" />

</Project>
