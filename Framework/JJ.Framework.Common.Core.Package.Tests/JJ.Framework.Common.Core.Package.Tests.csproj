    <PackageReference Include="JJ.Framework.Common.Legacy" Version="0.250.4821" />
    <PackageReference Include="JJ.Framework.IO.Core" Version="2.9.4454" />
    <PackageReference Include="JJ.Framework.Testing.Core" Version="2.9.4454" />
    <PackageReference Include="JJ.Framework.Testing.Legacy" Version="0.250.4457" />
