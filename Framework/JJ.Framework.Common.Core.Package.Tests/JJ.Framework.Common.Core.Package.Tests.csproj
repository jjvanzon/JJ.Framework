<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net10.0;net9.0;net8.0;net7.0;net6.0;net5.0;net461</TargetFrameworks>
    <Version>3.7.$(BuildNum)</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JJ.Framework.Common.Core" Version="3.7.4821" />
