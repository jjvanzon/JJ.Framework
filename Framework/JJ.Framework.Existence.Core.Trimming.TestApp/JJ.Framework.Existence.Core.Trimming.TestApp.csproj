<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>3.4.$(BuildNum)</Version>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net10.0;net9.0;net8.0;net7.0;net6.0;net5.0</TargetFrameworks>
    <RuntimeIdentifier>win-x64</RuntimeIdentifier>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\JJ.Framework.Existence.Core.Tests\**\*.cs" Link="%(RecursiveDir)%(Filename)%(Extension)" />
    <Compile Remove="..\JJ.Framework.Existence.Core.Tests\obj\**" />
    <Compile Remove="..\JJ.Framework.Existence.Core.Tests\bin\**" />
    <Compile Remove="..\JJ.Framework.Existence.Core.Tests\CrossRefCollectionTypesTests.cs" /> <!-- Removed from Trimming Test; Heavily relies on Reflection. -->
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\JJ.Framework.Existence.Core\JJ.Framework.Existence.Core.csproj" />
    <ProjectReference Include="..\JJ.Framework.Testing.Core\JJ.Framework.Testing.Core.csproj" />
  </ItemGroup>

  <Import Project="..\JJ.Framework.PlatformCompatibility.Core\JJ.Framework.PlatformCompatibility.Core.projitems" Label="Shared" />
  <Import Project="..\JJ.Framework.SharedProject.Core\JJ.Framework.SharedProject.Core.projitems" Label="Shared" />

</Project>
