<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Empty value aims to fall back to the operating system's culture. -->
    <add key="FixedCultureName" value="nl-NL" />
  </appSettings>

  <runtime>
    <!-- This would help UnhandledExceptionMessageBoxShower 
    prevent undhandled exceptions on non-IU thread from halting the application. -->
    <legacyUnhandledExceptionPolicy enabled="1" />
  </runtime>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
</configuration>